<android.support.v4.widget.DrawerLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/drawer_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    
    
    
    <!-- The main content view -->
    <FrameLayout
        android:id="@+id/activity_inicio_content_frame"
        android:layout_width="match_parent"
        android:layout_height="match_parent" >

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|top"
            android:layout_marginLeft="5dp"
            android:layout_marginRight="5dp">

        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="left|top"
            android:layout_marginTop="10dp"
            android:background="@color/cinza_platina"
            android:padding="5dp">

            <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="left|top">

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/tipo_acumulo_atacado"
                android:layout_weight="1"
                android:id="@+id/textView4"
                android:gravity="left" />

            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="@string/tipo_acumulo_atacado"
                android:layout_weight="2"
                android:id="@+id/activity_inicio_text_tipo_acumulo"
                android:gravity="right" />
        </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/valor_acumulado_atacado"
                    android:layout_weight="1"
                    android:id="@+id/textView5"
                    android:gravity="left" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/valor_acumulado_atacado"
                    android:layout_weight="2"
                    android:id="@+id/activity_inicio_text_valor_acumulado"
                    android:gravity="right" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal|bottom">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/prazo_acumulo_atacado"
                    android:layout_weight="1"
                    android:id="@+id/textView6"
                    android:gravity="left"
                    android:layout_gravity="center_horizontal|bottom" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/prazo_acumulo_atacado"
                    android:layout_weight="2"
                    android:id="@+id/activity_inicio_text_prazo_acumulado"
                    android:gravity="right"
                    android:layout_gravity="center_horizontal|bottom" />
            </LinearLayout>

            </LinearLayout>






        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="left|top"
            android:layout_marginTop="10dp"
            android:background="@color/cinza_platina"
            android:padding="5dp">

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="left|top">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/tipo_acumulo_varejo"
                    android:layout_weight="1"
                    android:id="@+id/textView7"
                    android:gravity="left" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/tipo_acumulo_varejo"
                    android:layout_weight="2"
                    android:id="@+id/activity_inicio_text_tipo_acumulo_varejo"
                    android:gravity="right" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/valor_acumulado_varejo"
                    android:layout_weight="1"
                    android:id="@+id/textView8"
                    android:gravity="left" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/valor_acumulado_varejo"
                    android:layout_weight="2"
                    android:id="@+id/activity_inicio_text_valor_acumulado_varejo"
                    android:gravity="right" />
            </LinearLayout>

            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal|bottom">

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/prazo_acumulo_varejo"
                    android:layout_weight="1"
                    android:id="@+id/textView9"
                    android:gravity="left"
                    android:layout_gravity="center_horizontal|bottom" />

                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="@string/prazo_acumulo_varejo"
                    android:layout_weight="2"
                    android:id="@+id/activity_inicio_text_prazo_acumulado_varejo"
                    android:gravity="right"
                    android:layout_gravity="center_horizontal|bottom" />
            </LinearLayout>



            </LinearLayout>


        </LinearLayout>

    </FrameLayout>
    
    <!-- The navigation drawer -->
    <ListView android:id="@+id/activity_inicio_left_drawer"
        android:layout_width="240dp"
        android:layout_height="match_parent"
        android:layout_gravity="start"
        android:choiceMode="singleChoice"
        android:divider="@android:color/transparent"
        android:dividerHeight="0dp"
        android:background="#ffff"/>
</android.support.v4.widget.DrawerLayout><!-- From: file:/D:/Dropbox/Forca%20de%20Venda%20Movel/Codigo%20Fonte/SAVARE_Studio/app/src/main/res/layout/activity_inicio.xml -->